#include <stdio.h>
#include <stdlib.h>

int main(int argc, char **av)
{
    if (argc < 2)
        exit(1);
    printf("Address for %s is %p\n", av[1], getenv(av[1]));
}

gcc /tmp/code.c -o /tmp/getenv

bonus0@RainFall:~$ /tmp/getenv SHELLCODE
